<Window x:Class="ExchangeClient.RegisterWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Регистрация клиента" Height="330" Width="300"
        Background="Black" Loaded="Window_Loaded">
    <StackPanel Margin="10">
        <Label>Введите логин:</Label>
        <TextBox Name="tbLogin" Margin="0 5"></TextBox>
        <Label>Введите логин к скайп:</Label>
        <TextBox Name="tbLoginSkype" Margin="0 5"></TextBox>
        <Label>Введите email:</Label>
        <TextBox Name="tbEmail" Margin="0 5"></TextBox>
        <Label>Введите пароль:</Label>
        <PasswordBox Name="tbPass" Margin="0 5"></PasswordBox>
        <Label>Повторить пароль:</Label>
        <PasswordBox Name="tbRepeatPass" Margin="0 5"></PasswordBox>
        <StackPanel Grid.Row="2" Orientation="Horizontal" Margin="0 5" DockPanel.Dock="Bottom">
            <Button Margin="0, 5, 10, 5" Padding="20, 2" Name="btnOk" Click="btnOk_Click">Ok</Button>
            <Button Margin="10, 5" Padding="10, 2" Name="btnCancel" Click="btnCancel_Click">Cancel</Button>
        </StackPanel>
    </StackPanel>
</Window>
